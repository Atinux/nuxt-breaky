{
  "engines": {
    "node": "10.x"
  },
  "name": "nuxt-breaky",
  "version": "0.1.0",
  "description": "A nuxt module for working with Tailwind CSS",
  "author": "Nattha",
  "homepage": "https://github.com/teamnovu/nuxt-breaky",
  "repository": {
    "type": "git",
    "url": "github:teamnovu/nuxt-breaky"
  },
  "license": "MIT",
  "private": false,
  "keywords": [
    "vue",
    "nuxt",
    "tailwindcss",
    "breakpoints",
    "plugin"
  ],
  "main": "./dist/js/module.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build-npm": "mkdir -p ./dist/js && mkdir -p ./dist/components && cp ./plugins/module.js ./dist/js/ && cp ./plugins/plugin.js ./dist/js/ && cp ./components/Breaky.vue ./dist/components/",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/tailwindcss": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2"
  }
}
